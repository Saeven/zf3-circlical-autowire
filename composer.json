{
    "name": "saeven/zf3-circlical-autowire",
    "description": "Annotation based routing, and magic Controller creation for ZF3.",
    "type": "library",
    "license": "MPL-2.0",
    "homepage": "https://github.com/Saeven/zf3-circlical-autowire",
    "keywords": [
        "router",
        "zf3",
        "annotation",
        "acl",
        "controller"
    ],
    "authors": [
        {
            "name": "Alexandre Lemaire",
            "email": "alemaire@circlical.com",
            "homepage": "http://circlical.com/",
            "role": "Paper Stapler"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.0",
        "laminas/laminas-mvc": "^3.0.1",
        "laminas/laminas-servicemanager": "^3.0.1",
        "laminas/laminas-eventmanager": "^3.0.1",
        "laminas/laminas-console": "@stable",
        "laminas/laminas-router": "@stable",
        "laminas/laminas-code": "@stable",
        "laminas/laminas-config": "@stable",
        "doctrine/annotations": "@stable",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "6.1.*",
        "laminas/laminas-form": "@stable",
        "friends-of-phpspec/phpspec-code-coverage": "@stable",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CirclicalAutoWire\\": "src/CirclicalAutoWire"
        }
    },
    "extra": {
        "zf": {
            "module": "CirclicalAutoWire"
        }
    }
}
